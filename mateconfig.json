{
    "builds": [{
            "name": "dev",
            "css": {
                "minify": false,
                "sourceMap": true
            },
            "js": {
                "minify": false,
                "sourceMap": true,
                "declaration": false,
                "webClean": true
            },
            "ts": {
                "compilerOptions": {
                    "noImplicitAny": false,
                    "noEmitOnError": false,
                    "removeComments": true,
                    "target": "es5",
                    "lib": ["ES2015", "DOM"]
                }
            }
        },
        {
            "name": "dist",
            "outDir": "dist",
            "outDirVersioning": true,
            "outDirName": false,
            "css": {
                "outDirSuffix": "css",
                "minify": true,
                "sourceMap": false
            },
            "js": {
                "minify": true,
                "sourceMap": false,
                "declaration": true,
                "webClean": true
            },
            "ts": {
                "compilerOptions": {
                    "noImplicitAny": false,
                    "noEmitOnError": false,
                    "removeComments": true,
                    "target": "es5",
                    "lib": ["ES2015", "DOM"]
                }
            }
        }
    ],
    "files": [{
        "output": ["test/js/cloudtranslation.js"],
        "input": [
            "src/enums.ts",
            "src/classes.ts",
            "src/Interfaces.ts",
            "src/main.ts"
        ],
        "builds": ["dev", "dist"]
    }]
}